{
  "realm": "myrealm",
  "enabled": true,
  "users": [
    {
      "username": "testuser",
      "enabled": true,
      "emailVerified": true,
      "credentials": [
        {
          "type": "password",
          "value": "testpass"
        }
      ]
    }
  ],
  "clients": [
    {
      "clientId": "my-spring-client",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "http://localhost:8080/*"
      ],
      "webOrigins": [
        "http://localhost:8080"
      ]
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "USER",
        "description": "Basic application user role"
      },
      {
        "name": "ADMIN",
        "description": "Application administrator role"
      }
    ]
  }
}
